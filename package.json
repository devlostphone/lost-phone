{
    "name": "lost-phone",
    "version": "1.0.0",
    "description": "A Lost-Phone Project using Parcel and TypeScript",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "build": "parcel build src/index.html --dist-dir dist --public-url ./",
        "watch": "parcel serve src/index.html -p 8000"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/devlostphone/lost-phone.git"
    },
    "author": "Lost Phone Developers",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/devlostphone/lost-phone/issues"
    },
    "homepage": "https://github.com/devlostphone/lost-phone",
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.9.3",
        "@parcel/packager-xml": "^2.9.3",
        "@types/node": "^20.5.0",
        "@types/qrcode": "^1.5.1",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "minimist": ">=1.2.8",
        "parcel": "^2.9.3",
        "parcel-config-precache-manifest": "^0.0.4",
        "parcel-config-pwa-manifest": "^0.1.2",
        "parcel-reporter-clean-dist": "^1.0.4",
        "parcel-reporter-static-files-copy": "^1.5.2",
        "phaser": "^3.70.0",
        "phaser3-rex-plugins": "^1.60.10",
        "process": "^0.11.10",
        "qrcode": "^1.5.3",
        "typescript": "^5.1.6"
    },
    "pwaManifest": {
        "name": "Lost & Phone",
        "shortName": "L&P",
        "startURL": "./index.html",
        "theme": "#add8e6",
        "generateIconOptions": {
            "baseIcon": "./static/themes/default/icons/podcast@133.png",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true
        }
    },
    "dependencies": {
        "phaser3-rex-plugins": "^1.60.4"
    },
    "overrides": {
        "@parcel/watcher": "2.1.0"
    }
}
