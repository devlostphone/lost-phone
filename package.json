{
    "name": "lost-phone",
    "version": "1.0.0",
    "description": "A Lost-Phone Project using Parcel and TypeScript",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "build": "parcel build src/index.html --dist-dir dist --public-url ./",
        "watch": "parcel serve src/index.html -p 8000"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/devlostphone/lost-phone.git"
    },
    "author": "Lost Phone Developers",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/devlostphone/lost-phone/issues"
    },
    "homepage": "https://github.com/devlostphone/lost-phone",
    "devDependencies": {
        "@parcel/packager-raw-url": "^2.4.1",
        "@parcel/packager-xml": "^2.4.1",
        "@types/node": "^17.0.25",
        "@types/qrcode": "^1.4.2",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "eslint": "^8.13.0",
        "minimist": ">=1.2.6",
        "parcel": "^2.4.1",
        "parcel-config-precache-manifest": "^0.0.4",
        "parcel-config-pwa-manifest": "^0.1.1",
        "parcel-reporter-clean-dist": "^1.0.4",
        "parcel-reporter-static-files-copy": "^1.3.4",
        "phaser": "^3.55.2",
        "qrcode": "^1.5.0",
        "typescript": "^4.6.3"
    },
    "pwaManifest": {
        "name": "Lost & Phone",
        "shortName": "L&P",
        "startURL": "./index.html",
        "theme": "#add8e6",
        "generateIconOptions": {
            "baseIcon": "./static/themes/default/icons/podcast.png",
            "sizes": [
                192,
                384,
                512
            ],
            "genFavicons": true
        }
    }
}
